:
\begin_inset Formula 
\begin{align*}
\frac{\partial f}{\partial x_{k}} & =2\sum_{i=1}^{m}\left(g_{i}\left(x\right)-d_{i}\right)\frac{\partial g_{i}}{\partial x_{k}}\\
 & =2\sum_{i=1}^{m}\left(g_{i}\left(x\right)-d_{i}\right)\frac{x_{k}-a_{ik}}{g_{i}\left(x\right)}\\
 & =2\sum_{i=1}^{m}\left(\left|\left|x-a_{i}\right|\right|_{2}-d_{i}\right)\frac{x_{k}-a_{ik}}{\left|\left|x-a_{i}\right|\right|_{2}}\\
 & =
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\nabla f\left(x\right) & =2\sum_{i=1}^{m}\left(\left|\left|x-a_{i}\right|\right|_{2}-d_{i}\right)\frac{\left(x-a_{i}\right)}{\left|\left|x-a_{i}\right|\right|_{2}}\\
 & =2\sum_{i=1}^{m}\left(x-a_{i}\right)-2\sum_{i=1}^{m}d_{i}\frac{\left(x-a_{i}\right)}{\left|\left|x-a_{i}\right|\right|_{2}}\underbrace{=}_{\text{demand}}0\\
\Rightarrow & \sum_{i=1}^{m}\left(x-a_{i}\right)-\sum_{i=1}^{m}d_{i}\frac{\left(x-a_{i}\right)}{\left|\left|x-a_{i}\right|\right|_{2}}=0\\
\Rightarrow & mx-\sum_{i=1}^{m}a_{i}-\sum_{i=1}^{m}d_{i}\frac{\left(x-a_{i}\right)}{\left|\left|x-a_{i}\right|\right|_{2}}=0\\
\Rightarrow & \boxed{x=\frac{1}{m}\left(\sum_{i=1}^{m}a_{i}+\sum_{i=1}^{m}d_{i}\frac{\left(x-a_{i}\right)}{\left|\left|x-a_{i}\right|\right|_{2}}\right)}
\end{align*}

